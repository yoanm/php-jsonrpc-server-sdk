{
    "name": "yoanm/jsonrpc-server-sdk",
    "description": "Server SDK to convert a json-rpc request string into json-rpc response string",
    "license": "GPL-3.0-only",
    "type": "library",
    "support": {
        "issues": "https://github.com/yoanm/php-jsonrpc-server-sdk/issues"
    },
    "authors": [
        {
            "name": "Yoanm",
            "email": "yoanm@users.noreply.github.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Yoanm\\JsonRpcServer\\App\\": "src/App",
            "Yoanm\\JsonRpcServer\\Domain\\": "src/Domain",
            "Yoanm\\JsonRpcServer\\Infra\\": "src/Infra"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests",
            "Tests\\Functional\\BehatContext\\": "features/bootstrap"
        }
    },
    "require": {
        "php": ">7.0"
    },
    "suggest": {
        "symfony/dependency-injection": "Better sdk integration. Method resolver and json-rpc methods tags for autoloading"
    },
    "require-dev": {
        "behat/behat": "~3.0",
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": "^6.0"
    }
}
